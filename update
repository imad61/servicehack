print(e);
}
if(GUI!=null&&GUI.isShowing()==false&&(hackermenu==null||hackermenu.isShowing()==false))showmenubtn();
/*if(!GUI && !hackermenu)showMenuBtn();*/
                        eval(every());
                    }
                }), 1000 / 70);
        }
    }))
}
