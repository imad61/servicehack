var a = 0.5+Entity.getX(mobs[i]);

var b = Entity.getY(mobs[i]);

var c = 0.5+Entity.getZ(mobs[i]);

var len = Math.sqrt(x*x + y*y + z*z); 

var y = y/len; 

var pitch = Math.asin(y);
pitch = pitch * 180.0 / Math.PI;
pitch=-pitch;
if(pitch<90&&pitch>-90){
aimAt(mobs[i]);

}
}
}
}
